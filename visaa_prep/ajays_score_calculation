# Read the number of test cases
T = int(input().strip())

# Process each test case
for _ in range(T):
    # Read X and N
    X, N = map(int, input().strip().split())
    
    # Calculate the score
    score_per_case = X // 10
    total_score = score_per_case * N
    
    # Print the result
    print(total_score)
